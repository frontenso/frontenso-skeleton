{
  "name": "skeleton",
  "version": "1.0.0",
  "license": "MIT",
  "description": "A template for static markup projects.",
  "scripts": {
    "start": "cross-env NODE_ENV=development npm-run-all --parallel gulp webpack",
    "build": "cross-env NODE_ENV=production npm-run-all gulp:production webpack",
    "zip": "cross-env NODE_ENV=production gulp zip",
    "format": "pretty-quick --staged",
    "lint:css": "stylelint \"src/**/*.{scss,sass}\"",
    "lint:js": "eslint --max-warnings 0 \"src/**/*.{jsx,js}\"",
    "validate": "npm-run-all --parallel format lint:css lint:js",
    "webpack": "webpack --config webpack.config.js",
    "gulp": "gulp",
    "gulp:production": "gulp production"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run validate"
    }
  },
  "dependencies": {
    "ansi-colors": "^4.1.1",
    "assets": "^3.0.1",
    "autoprefixer": "^10.2.4",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "fancy-log": "^1.3.3",
    "global": "^4.4.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-jsbeautifier": "^3.0.1",
    "gulp-minify-inline-scripts": "^0.0.6",
    "gulp-nunjucks-api": "^0.9.1",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-rimraf": "^1.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-svgmin": "^3.0.0",
    "gulp-svgstore": "^7.0.1",
    "gulp-w3c-html-validator": "^2.0.1",
    "gulp-zip": "^5.0.2",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.1",
    "imports-loader": "^1.2.0",
    "json-loader": "^0.5.7",
    "md5-file": "^4.0.0",
    "nanoid": "^3.1.20",
    "node-notifier": "^8.0.0",
    "postcss": "^8.2.4",
    "postcss-assets": "^5.0.0",
    "postcss-sprites": "^4.2.1",
    "raw-loader": "^4.0.0",
    "through2": "^4.0.2",
    "vinyl-named": "^1.1.0",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0",
    "yargs": "^16.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "browser-sync": "^2.26.13",
    "eslint": "^7.12.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jsdoc": "^32.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-watch": "^5.0.1",
    "husky": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "stylelint": "^13.7.2",
    "stylelint-config-recommended": "^3.0.0",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.5"
  }
}
